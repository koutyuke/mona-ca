{
  "name": "@mona-ca/tailwindcss",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./merge": "./src/tailwind-merge.ts",
    "./variants": "./src/tailwind-variants.ts",
    "./input.css": "./input.css"
  },
  "scripts": {
    "fmt": "biome format --no-errors-on-unmatched",
    "fmt:fix": "biome format --write --no-errors-on-unmatched",
    "lint": "biome lint --error-on-warnings --no-errors-on-unmatched",
    "lint:fix": "biome lint --apply --no-errors-on-unmatched"
  },
  "dependencies": {
    "@mona-ca/design-tokens": "workspace:*",
    "tailwind-merge": "^2.4.0",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@mona-ca/tsconfig": "workspace:*",
    "tailwindcss": "^3.4.3"
  }
}
