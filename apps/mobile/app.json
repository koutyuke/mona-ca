{
  "expo": {
    "name": "mona-ca",
    "slug": "mona-ca",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mona-ca",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mona-ca.mobile",
      "icon": {
        "light": "./assets/images/icon.png",
        "dark": "./assets/images/icon-dark.png",
        "tinted": "./assets/images/icon-tinted.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFB7AE"
      },
      "package": "com.monaca.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "../../node_modules/@expo-google-fonts/kiwi-maru/300Light/KiwiMaru_300Light.ttf",
            "../../node_modules/@expo-google-fonts/kiwi-maru/400Regular/KiwiMaru_400Regular.ttf",
            "../../node_modules/@expo-google-fonts/kiwi-maru/500Medium/KiwiMaru_500Medium.ttf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "backgroundColor": "#FFB7AE",
          "dark": {
            "image": "./assets/images/splash-icon-dark.png",
            "backgroundColor": "#1C1C1E"
          },
          "imageWidth": 160
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    }
  }
}
